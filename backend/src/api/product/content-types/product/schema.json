{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "most_selling": {
      "type": "boolean"
    },
    "price": {
      "type": "text"
    },
    "pieces": {
      "type": "biginteger"
    },
    "price_formatting": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "part1",
        "part2",
        "part3"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "feature": {
      "type": "boolean"
    },
    "rate": {
      "type": "biginteger"
    },
    "slug": {
      "type": "uid"
    }
  }
}
